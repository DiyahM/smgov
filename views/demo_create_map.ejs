 </div>

    <div style="margin-top:40px;">
      <div class="row-fluid">
        <div class="span9">
          
          
		     <div class="row-fluid">
	            <div id="map_canvas">Loading map...</div>
	          </div>
			<div class="row-fluid">
	            <div class="control-group">

			        <div class="controls" style="padding-top:6px;">
			          <label class="checkbox inline"><strong>Data Views:</strong> 
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="camera_checkbox" value="">Traffic Cameras
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="twitter_sentiment_checkbox" value="">Sentiment
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="twitter_sentiment_checkbox" value="">Images
				      </label>
				    
			          <label class="checkbox inline"><strong>Map Overlays:</strong> 
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="police_checkbox" value="">Police Stations
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="fire_dept_checkbox" value="">Fire Departments
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="hospital_checkbox" value="">Hospitals
				      </label>
				      
				      <button type="button" class="btn btn-mini" style="margin-left:8px;"><i class="icon-map-marker"></i>Upload Map Overlays File</button> 
				    </div>
			      </div>
	         <div class="row-fluid">
	           <!--DATA TABLE Begin -->
	           <div id="data_table" style="max-height:235px;overflow:auto;margin-left:5px;">
	             <table class="table table-condensed table-bordered table-striped">
	               <thead>
	                 <tr>
	                   <th>Keyword</th>
	                   <th>Source</th>
	                   <th>Items</th>
	                   <th>Images</th>
	                   <th>Video</th>
	                   <th>Geocoded</th>
	                   <th>Sentiment</th>
	                   <th>Actions</th>
	                 </tr>
	               </thead>
	             </table>
	            </div>
	           <!-- DATA TABLE END -->
	           <!--TEMP for debug-->
	           <div id="tweets"><ul></ul></div>
	           <!--sample tweet that will go to 140 chars in 0 to 140. sample text, please help me. my throat is sore. still have 29 chars left at this now 4 1-->
	         </div>
          </div>
        </div>
        <div class="span3 offset2">
           	<!-- Forms
			================================================== --> 
           <div class="row-fluid">
			<section id="forms">
			      <form class="well">
			        
			        <fieldset>
			          
			          <h5><i class="icon-search"></i> SEARCH</h5><br>
			         
			         <!--Social Media Search Begin--> 
			         <div class="control-group">
				        <div class="controls">
				          	<input type="text" class="input-large" id="keyword_input" placeholder="New keyword"> <button type='button' class='btn' onclick='addKeyword()' style='margin-top:-10px;'><i class="icon-plus"></i></button>
							  <div id="keywords"></div>
						  <button type="button" class="btn btn-mini" style="">Upload Keyword File</button><br><br>
					      <label class="checkbox inline">
					        <input type="checkbox" id="twitter_checkbox" value="">Twitter
					      </label>
					      <label class="checkbox inline">
					        <input type="checkbox" id="facebook_checkbox" value="">Facebook
					      </label>
				          <label class="checkbox inline">
					        <input type="checkbox" id="rss_checkbox" value="">RSS
					      </label>
					      <label class="checkbox">
					        <input type="checkbox" id="custom_checkbox" value="">{Custom User Defined}
					      </label>
					      
					    </div>
				      </div>
				      	<button type="button" class="btn btn-mini" style="">Record Data</button>
					      <button type="button" class="btn btn-mini btn-primary" style="">Publish Map</button>
				     </form>
				     </fieldset>
				      <!-- Social Media END -->
				      <!--MAP LAT LON BEGIN-->
				     <form class="well">

				        <fieldset>
				     <h5> <i class="icon-map-marker"></i>LOCATION</h5><br>
			          <div class="control-group">

			            <div class="controls">
			              
			              <div class="input-prepend">
							<span class="add-on">
							  <i class="icon-map-marker"></i>
							</span>
			              <input type="text" class="input-large" id="map_coordinates" >
			              </div>
			              Use map controls to reposition. Or enter address, city, state.
			            </div>
			          </div>
			          
			         <!--LAT LON END-->
				      
			        </fieldset>
			      </form>
			</section>
			</div>
			<!-- Report
			================================================== --> 
           <div class="row-fluid">
			<section id="report">
			      <form class="well form-horizontal">

			        <fieldset>
			        
			         <i class="icon-envelope"></i> SPOT REPORT
			          
			          <div class="control-group" style="margin-top:15px;">
			             <label class="control-label" for="send_to" style="width:auto;">TO: </label>
			           	<div class="controls" style="margin-left:30px;">
			              <input type="text" class="input-large" id="send_to" placeholder="Add Recipient" >
			            </div> 
			           <div class="control-group" style="margin-top:15px;">
			              <label class="control-label" for="report_description" style="width:auto;">RE: </label>
			            <div class="controls" style="margin-left:30px;">
			              <input type="text" class="input-large" id="report_description" placeholder="Report Description">
			            </div>
			           </div>
			            <span class="badge badge-info">0</span> ATTACHMENTS
			          </div>
				      
                       <p>
				        <button type='button' class='btn btn-mini'>Create in Report View</button> <button type='button' class='btn btn-mini btn-primary'>Save Report</button></p>
				      Drag elements to this box to attach to report

			        </fieldset>
			      </form>
			</section>
			</div>
        </div>
      </div>
      





<br><br><br><br>

     <!-- Footer
      ================================================== -->
      <footer class="footer">
