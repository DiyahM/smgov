   </div>
 </div>

    <div style="margin-top:40px;">
      <div class="row-fluid">
        <div class="span9">
          
          
		     <div class="row-fluid">
	            <div id="map_canvas">Loading map...</div>
	          </div>
			<div class="row-fluid">
	            <div class="control-group">

			        <div class="controls" style="padding-top:6px;">
			          <label class="checkbox inline"><strong>Map Options:</strong> 
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="camera_checkbox" value="">Traffic Cameras
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="twitter_sentiment_checkbox" value="">Tweet Sentiment
				      </label>
			          <label class="checkbox inline">
				        <input type="checkbox" id="gov_bldg_checkbox" value="">Government Buildings
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="police_checkbox" value="">Police Stations
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="fire_dept_checkbox" value="">Fire Departments
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="hospital_checkbox" value="">Hospitals
				      </label>
				      <button type="button" class="btn btn-mini" style="margin-left:8px;"><i class="icon-map-marker"></i>Upload Map Overlays File</button> 
				    </div>
			      </div>
	         <div class="row-fluid">
	           <div id="tweets" style="max-height:235px;overflow:auto;"><ul></ul></div>
	           <!--sample tweet that will go to 140 chars in 0 to 140. sample text, please help me. my throat is sore. still have 29 chars left at this now 4 1-->
	         </div>
          </div>
        </div>
        <div class="span3 offset2">
           	<!-- Forms
			================================================== --> 
           <div class="row-fluid">
			<section id="forms">
			      <form class="well">
			        
			        <fieldset>
			          
			          <p><i class="icon-cog"></i> SETTINGS</p>
			         
			         <!--Social Media Search Begin--> 
			         <div class="control-group">
				        <div class="controls">
				          	<input type="text" class="input-large" id="keyword_input" placeholder="New keyword"> <button type='button' class='btn' onclick='addKeyword()' style='margin-top:-10px;'>Add</button>
							  <div id="keywords"></div>
					      <label class="checkbox inline">
					        <input type="checkbox" id="twitter_checkbox" value="">Twitter
					      </label>
					      <label class="checkbox inline">
					        <input type="checkbox" id="facebook_checkbox" value="">Facebook
					      </label>
				          <label class="checkbox inline">
					        <input type="checkbox" id="rss_checkbox" value="">RSS
					      </label>
					      <label class="checkbox">
					        <input type="checkbox" id="custom_checkbox" value="">Custom User Defined Stream
					      </label>
					      
					    </div>
				      </div>
				      <!-- Social Media END -->
				      <!--MAP LAT LON BEGIN-->
			          <div class="control-group">

			            <div class="controls">
			              <p class="help-block">Use map controls to reposition. Or enter address, city, state in below.</p>
			              <div class="input-prepend">
							<span class="add-on">
							  <i class="icon-map-marker"></i>
							</span>
			              <input type="text" class="input-large" id="map_coordinates" >
			              </div>
			              
			            </div>
			          </div>
			          	<button type="button" class="btn btn-mini btn-primary" style="">Publish Stream</button>
					      <button type="button" class="btn btn-mini btn-primary" style="">Record Stream</button>
			         <!--LAT LON END-->
				      
			        </fieldset>
			      </form>
			</section>
			</div>
			<!-- Report
			================================================== --> 
           <div class="row-fluid">
			<section id="report">
			      <form class="well">

			        <fieldset>
			        
			         <p><i class="icon-envelope"></i> SPOT REPORT</p>
			         
			          <p>Drag and Drop elements to this box to attach to report</p>
			          <div class="control-group">
			           	<div class="controls">
			              <label class="inline">TO: </label>
			              <input type="text" class="input-medium" id="send_to" placeholder="Add Recipient">
			            </div> 
			            <div class="controls">
			              <label class="inline">RE: </label>
			              <input type="text" class="input-medium" id="report_description" placeholder="Report Description">
			            </div>
			            ATTACHMENTS: <span class="label">0</span>
			          </div>
			          <center>
				      
                       <p>
				        <button type='button' class='btn'>Create in Report View</button> <button type='button' class='btn'>Save Report</button></p></center>

			        </fieldset>
			      </form>
			</section>
			</div>
        </div>
      </div>
      





<br><br><br><br>

     <!-- Footer
      ================================================== -->
      <footer class="footer">
        <p class="pull-right">2012 SNAP Link -- For Demo Only</p>
