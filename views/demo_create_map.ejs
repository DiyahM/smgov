 </div>
<!----END NAVBAR -->

<!---BEGIN CONTAINER-->

    <div class="container" style="margin-top:40px;">
      <!--MAIN CONTENT BEGIN-->
      <div class="row-fluid">
        <!---BEGIN COLUMN 1 contains MAP, Overlay Checkboxes, and Results Table-->
        <div class="span9">
          
         <!--MAP BEGIN--> 
		     <div class="row-fluid">
	            <div id="map_canvas" class="halfscreen">Loading map...</div>
	          </div>
	     <!--MAP END--> 
	     <!--Overlays BEGIN-->
			<div class="row-fluid">
			    
	            <div class="control-group map_overlays">

			        <div class="controls" style="padding-top:6px;">
			          <label class="checkbox inline"><strong>Data Layers:</strong> 
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="camera_checkbox" value="" onclick="cameraToggle()" checked>Traffic Cameras (DC Area Only)
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="geo_checkbox" value="" onclick="geoToggle()" checked>Geocoded Tweets
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="twitter_sentiment_checkbox" value="" disabled>Images
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="twitter_sentiment_checkbox" value="" disabled>Twitter Sentiment
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="police_checkbox" value="" disabled>Police Stations
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="fire_dept_checkbox" value="" disabled>Hospitals
				      </label>
				      <label class="checkbox inline">
				        <input type="checkbox" id="hospital_checkbox" value="" disabled>{Custom Defined}
				      </label>
				      
				      <button type="button" class="btn btn-mini disabled" style="margin-left:8px;"><i class="icon-map-marker"></i>Upload Map Overlays File</button> 
				    </div>
			      </div>
			</div>
			<!--Overlays END-->
			<!--Results table Begin -->
	         <div class="row-fluid">
	           
	           <div id="data_table" style="max-height:235px;overflow:auto;margin-left:5px;visibility:hidden;">
	             <table class="table table-condensed table-bordered table-striped" id="tweets_table">
	               <thead>
	                 <tr>
	                   <th>Source</th>
	                   <th>Keyword</th>
	                   <th>Items</th>
	                   <th>Images</th>
	                   <th>Video</th>
	                   <th>Geocoded</th>
	                   <th>Positive</th>
	                   <th>Negative</th>
	                   <th>Neutral</th>
	                 </tr>
	               </thead>
	             </table>
	            </div>
	          </div>
	           <!-- Results table END -->
	           <!--TEMP for debug BEGIN-->
	           <div id="tweets"><ul></ul></div>
	           <!--sample tweet that will go to 140 chars in 0 to 140. sample text, please help me. my throat is sore. still have 29 chars left at this now 4 1-->
	          <!--TEMP for debug END-->
	         </div>
	     <!--COLUMN1 END-->
        <!--COLUMN2 BEGIN for search, location, and spot report-->  
        <div class="span3 offset2">
          <!--Record button begin -->
          <div class="row-fluid"><button class="btn disabled" style="width:95%;margin:10px;">Record Data Stream</button></div>
           <!--Record button end -->
        <!--SEARCH BEGIN-->
           <div class="row-fluid">
			
			      <form class="well">
			        
			        <fieldset>
			          
			          <h5><i class="icon-search"></i> SEARCH</h5><br>
			         
			         
			         <div class="control-group">
				        <div class="controls">
				          	<input type="text" class="input-medium" id="keyword_input" placeholder="New keyword"> <button type='button' class='btn' onclick='addKeyword()' style='margin-top:-10px;'><i class="icon-plus"></i></button>
							  <div id="keywords"></div>
						  <button type="button" class="btn btn-mini disabled" style="">Upload Keyword File</button><br><br>
					      <label class="checkbox inline">
					        <input type="checkbox" id="twitter_checkbox" value="" onclick="twitterToggle()" checked>Twitter
					      </label>
					      <label class="checkbox inline">
					        <input type="checkbox" id="facebook_checkbox" value="" onclick="facebookToggle()" checked>Facebook
					      </label>
				          <label class="checkbox inline">
					        <input type="checkbox" id="rss_checkbox" value="" onclick="rssToggle()" disabled>RSS
					      </label>
					      <label class="checkbox">
					        <input type="checkbox" id="custom_checkbox" value="" disabled>{Custom User Defined}
					      </label>
					      
					    </div>
				      </div>
				       <button type='button' class='btn disabled'>Save Search</button> <button type='button' class='btn btn-primary' onclick="search()">Search</button> 
				  </fieldset>   
				  </form>
				</div>
				<!--SEARCH END-->
				      <!--MAP LAT LON BEGIN-->
				  <div class="row-fluid">
				     <form class="well">

				        <fieldset>
				     <h5> <i class="icon-map-marker"></i>LOCATION</h5>
				<h6>Use map controls to reposition. Or enter address, city, state.</h6>
			          <div class="control-group">

			            <div class="controls">
			              
			              <div class="input-prepend">
							<span class="add-on">
							  <i class="icon-map-marker"></i>
							</span>
			              <input type="text" class="input-medium" id="map_coordinates" >
			              </div>
			              
			             <label class="checkbox">
					        <input type="checkbox" id="twitter_trends_checkbox" value="" disabled>Show Twitter Trends For Location
					      </label>
			            </div>
			          </div>
			          
			          
			         
				      
			        </fieldset>
			      </form>
			
			</div>
			<!--LAT LON END-->
			<!-- Report BEGIN
			================================================== --> 
           <div class="row-fluid">
			
			      <form class="well form-horizontal">

			        <fieldset>
			        
			         <h5><i class="icon-envelope"></i> SPOT REPORT</h5>
			          <h6>Drag elements to this box to attach to report</h6>
			          <div class="control-group" style="margin-top:15px;">
			             <label class="control-label" for="send_to" style="width:auto;">TO: </label>
			           	<div class="controls" style="margin-left:30px;">
			              <input type="text" class="input-medium" id="send_to" placeholder="Add Recipient" >
			            </div> 
			           <div class="control-group" style="margin-top:15px;">
			              <label class="control-label" for="report_description" style="width:auto;">RE: </label>
			            <div class="controls" style="margin-left:30px;">
			              <input type="text" class="input-medium" id="report_description" placeholder="Report Description">
			            </div>
			           </div>
			            <span class="badge badge-info">0</span> ATTACHMENTS <button type='button' class='btn btn-primary pull-right disabled' style="margin-top:-8px;margin-right:30px;">SEND</button>
			          </div>
				      

			        </fieldset>
			      </form>
			
			</div>
			<!--REPORT END-->
        </div>
      <!--COLUMN 2 END-->
      </div>
     <!--MAIN CONTENT END-->

     <!-- Footer
      ================================================== -->
      <footer class="footer">
        <p class="pull-right">2012 SNAP Link -- For Demo Only</p>
      </footer>

    </div><!-- /container end-->



